<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype.id), method: :get %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title, method: :get %>
    <p class="card__summary">
      <%= link_to prototype.catch_copy %>
    </p>
    <%= link_to "by"+ prototype.user.name, user_path(prototype.user_id), method: :get, class: :card__user %>
    <%# prototype.user.nameの名前表示, ()の名前の指定 %>
  </div>
</div>